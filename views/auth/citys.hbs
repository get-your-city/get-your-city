<a href="/">Home</a>

<h1>Citys to visit</h1>

{{#each citysArr}}
    <section class="">
        <h2>{{this.name}}</h2>
        <p>
            Country: {{this.country}} <br />
            Description: {{this.description}} <br />
            Image: {{this.image}} <br />
        </p>
    
        <a href="/auth/{{this._id}}/edit">Edit</a>

        <form action="/auth/{{this._id}}/delete" method="POST" >
            <button>Delete</button>
            </form>

    </section>
{{/each}}